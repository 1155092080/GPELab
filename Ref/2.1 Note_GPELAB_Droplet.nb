 FormBox[
  RowBox[{
   RowBox[{"T", "=", 
    RowBox[{"\[Omega]", " ", "t"}]}], ",", " ", 
   RowBox[{
    OverscriptBox["R", "\[RightVector]"], "=", 
    RowBox[{
     OverscriptBox["r", "\[RightVector]"], "/", "a"}]}], ",", " ", 
   RowBox[{"\[CapitalPsi]", "=", 
    RowBox[{"\[Psi]", " ", 
     SuperscriptBox["a", 
      RowBox[{"3", "/", "2"}]]}]}], ",", " ", 
   RowBox[{"a", "=", 
