 FormBox[
  RowBox[{
   RowBox[{
    SubscriptBox["g", "11"], "=", 
    FractionBox[
     RowBox[{"4", "\[Pi]", " ", 
      SuperscriptBox["\[HBar]", "2"], 
      SubscriptBox["a", "11"]}], 
     SubscriptBox["m", "1"]]}], ",", " ", 
   RowBox[{
    SubscriptBox["g", "22"], "=", 
    FractionBox[
     RowBox[{"4", "\[Pi]", " ", 
      SuperscriptBox["\[HBar]", "2"], 
      SubscriptBox["a", "22"]}], 
