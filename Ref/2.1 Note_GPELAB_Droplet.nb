  3.785241495364939*^9}},ExpressionUUID->"53e002d1-ea73-4c22-9731-\
a631ff97de67"],

Cell[BoxData[{
 FormBox[
  RowBox[{
   SubscriptBox["\[ScriptCapitalE]", "LHY"], "=", 
   RowBox[{
    FractionBox["8", 
     RowBox[{"15", 
      SuperscriptBox["\[Pi]", "2"]}]], 
    FractionBox[
     SuperscriptBox[
      RowBox[{
       SubsuperscriptBox["m", "1", 
        RowBox[{"3", "/", "2"}]], "(", 
       RowBox[{
        SubscriptBox["g", "11"], 
        SubscriptBox["n", "1"]}], ")"}], 
      RowBox[{"5", "/", "2"}]], 
     SuperscriptBox["\[HBar]", "3"]], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"1", "+", 
       RowBox[{
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox[
           SubscriptBox["m", "2"], 
           SubscriptBox["m", "1"]], ")"}], 
         RowBox[{"3", "/", "5"}]], 
        FractionBox[
         RowBox[{
          SubscriptBox["g", "22"], 
          SubscriptBox["n", "2"]}], 
         RowBox[{
          SubscriptBox["g", "11"], 
          SubscriptBox["n", "1"]}]]}]}], ")"}], 
     RowBox[{"5", "/", "2"}]]}]}], TraditionalForm], "\[LineSeparator]", 
 FormBox[
  RowBox[{"=", 
   RowBox[{
    FractionBox[
     RowBox[{"256", 
      SqrtBox["\[Pi]"], 
      SuperscriptBox["\[HBar]", "2"]}], "15"], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        SubsuperscriptBox["m", "1", 
         RowBox[{
          RowBox[{"-", "2"}], "/", "5"}]], 
        SubscriptBox["a", "11"], 
        SubscriptBox["n", "1"]}], "+", 
       RowBox[{
        SubsuperscriptBox["m", "2", 
         RowBox[{
          RowBox[{"-", "2"}], "/", "5"}]], 
        SubscriptBox["a", "22"], 
        SubscriptBox["n", "2"]}]}], ")"}], 
     RowBox[{"5", "/", "2"}]]}]}], TraditionalForm]}], "EquationNumbered",
 CellChangeTimes->{{3.7852336957465754`*^9, 3.785233749989566*^9}, {
  3.7852415083437486`*^9, 3.785241554271491*^9}, {3.7852416185969114`*^9, 
