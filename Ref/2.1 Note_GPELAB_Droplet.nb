 FormBox[
  RowBox[{
   FractionBox[
    SubscriptBox["\[ScriptCapitalE]", "LHY"], 
    RowBox[{"\[HBar]", " ", 
     RowBox[{"\[Omega]", "/", 
      SuperscriptBox["a", "3"]}]}]], "=", 
   RowBox[{
    FractionBox[
     RowBox[{"256", 
      SuperscriptBox["\[Pi]", 
       RowBox[{"1", "/", "2"}]]}], "15"], 
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{
       RowBox[{
        FractionBox[
         RowBox[{
          SubscriptBox["N", "1"], 
          SubscriptBox["a", "11"]}], "a"], 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["m", 
           SubscriptBox["m", "1"]], ")"}], 
         RowBox[{"2", "/", "5"}]], 
        SubsuperscriptBox["\[CapitalPsi]", "1", "*"], 
        SubscriptBox["\[CapitalPsi]", "1"]}], "+", " ", 
       RowBox[{
        FractionBox[
         RowBox[{
          SubscriptBox["N", "2"], 
          SubscriptBox["a", "22"]}], "a"], 
        SuperscriptBox[
         RowBox[{"(", 
          FractionBox["m", 
           SubscriptBox["m", "2"]], ")"}], 
         RowBox[{"2", "/", "5"}]], 
        SubsuperscriptBox["\[CapitalPsi]", "2", "*"], 
        SubscriptBox["\[CapitalPsi]", "2"]}]}], ")"}], 
     RowBox[{"5", "/", "2"}]]}]}], TraditionalForm]], "EquationNumbered",
